dirID=$1


rm data logs data_root unpacker_output vmeraw
mkdir /disk${dirID}_2TB/October2018
mkdir /disk${dirID}_2TB/October2018/raw
mkdir /disk${dirID}_2TB/October2018/logs
mkdir /disk${dirID}_2TB/October2018/onlineMon
mkdir /disk${dirID}_2TB/October2018/vmeraw
mkdir /disk${dirID}_2TB/October2018/unpacked
