ln -s /disk${dirID}_2TB/October2018/logs logs
ln -s /disk${dirID}_2TB/October2018/onlineMon data_root
ln -s /disk${dirID}_2TB/October2018/unpacked unpacker_output
ln -s /disk${dirID}_2TB/October2018/vmeraw vmeraw
